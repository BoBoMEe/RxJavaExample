<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bobomee.android.rxjavaexample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".util.RxApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Create.Creating">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".Transform.Transforming">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".Filter.Filtering">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".Subject.Subjects">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".Subject.SingleObservable">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".Combine.Combining">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
        <activity android:name=".ErrorHandle.ErrorHandling">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="com.bobomee.android.rxjavaexample.category" />
            </intent-filter>
        </activity>
    </application>

</manifest>
